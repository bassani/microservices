    "@types/chart.js": "^2.9.34",
    "@types/cucumber": "^7.0.0",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.21",
    "@types/node": "^12.11.1",
    "chai": "^4.3.4",
