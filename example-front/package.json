{
  "name": "purchasesimulatorfront",
  "version": "1.11.58",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "gen": "ng g c simulator/pages/simulation-result",
    "build": "ng build",
    "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --c prod --build-optimizer --aot ",
    "build:qa": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --c qa --build-optimizer --aot",
    "build:dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --c dev --build-optimizer --aot",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "jest",
    "test:watch": "jest --watchAll --collect-coverage",
    "test:ci": "jest --runInBand --collect-coverage",
    "test:dev": "jest --runInBand --collect-coverage",
    "sonar": "sonar-scanner -D sonar.projectKey=purchase-simulator-front -D sonar.sources=./src  -D sonar.host.url=http://localhost:9001 -D sonar.login=e08bb148e5b526f034afc9911429ba6c4f320d01"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "coveragePathIgnorePatterns": [
      ".mock.ts",
      "matchers.ts",
      ".html",
      "index.ts"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.9",
